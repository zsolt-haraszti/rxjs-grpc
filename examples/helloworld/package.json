{
  "name": "rxjs-grpc-helloworld",
  "version": "0.0.1",
  "description": "Helloworld example for rxjs-grpc",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kondi/rxjs-grpc.git"
  },
  "scripts": {
    "start": "npm run grpc-namespaces",
